@{
    ViewBag.Title="Vehicle-In Entry Form";
}

<div id="page-wrapper">
          
           <div class="row">
                <!-- Page Header -->
                <div class="col-lg-12">
                    <h1  class="page-header">@ViewBag.Title</h1>
                    
                </div>
                <!--End Page Header -->
            </div>
            <!--forms--->
 
 <!-- Row start -->
  <div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
      <div class="panel panel-default">
        <div class="panel-heading clearfix">
          <i class="icon-calendar"></i>
          <h3 class="panel-title">Vehicle-In</h3>
        </div>
        
        <div class="panel-body">

            <div class="row">
                <div class="col-lg-6 col-sm-6 col-sx-6">
                    <div class="form-group">

                        <input type="text" class="form-control" placeholder="Vehicle No">
                    </div>
                </div>
                <div class="col-lg-6 col-sm-6 col-sx-6">
                    <div class="form-group">
                        <input type="text" class="form-control" placeholder="Bilty No/Bulker No">

                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-lg-6 col-sm-6 col-sx-6">
                    <div class="form-group">

                        <input type="text" class="form-control" placeholder="Quantity Per MT">
                    </div>
                </div>
                <div class="col-lg-6 col-sm-6 col-sx-6">
                    <div class="form-group">
                        <input type="text" class="form-control" placeholder="Invoice No.">

                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-lg-6 col-sm-6 col-sx-6">
                    <div class="form-group">

                        <input type="text" class="form-control" placeholder="Party Name/Transporter Name">
                    </div>
                </div>
                <div class="col-lg-6 col-sm-6 col-sx-6">
                    <div class="form-group">
                        <input type="text" class="form-control" placeholder="Transport Name">

                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-6 col-sm-6 col-sx-6">
                    <div class="form-group">

                        <input type="text" class="form-control" placeholder="Rate Per Month">
                    </div>
                </div>
                <div class="col-lg-6 col-sm-6 col-sx-6">
                    <div class="form-group">
                        <input type="text" class="form-control" placeholder="Amount">

                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-lg-6 col-sm-6 col-sx-6">
                    <div class="form-group">

                        <input type="text" class="form-control" placeholder="Driver Name">
                    </div>
                </div>
                <div class="col-lg-6 col-sm-6 col-sx-6">
                    <div class="form-group">
                        <input type="text" class="form-control" placeholder="Driver Contact No">

                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-lg-6 col-sm-6 col-sx-6">
                    <div class="form-group">

                        <input type="text" class="form-control" placeholder="Party Name/Transporter Name">
                    </div>
                </div>
                <div class="col-lg-6 col-sm-6 col-sx-6">
                    <div class="form-group">
                        <input type="text" class="form-control" placeholder="Transport Name">

                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-6 col-sm-6 col-sx-6">
                    <div class="form-group">

                        <input type="text" class="form-control" placeholder="Remark">
                    </div>
                </div>
                <div class="col-lg-6 col-sm-6 col-sx-6">
                    <button class="btn btn-success pull-right">Save <i class="fa fa-save"></i></button>
                </div>
            </div>


        </div>
      </div>
    </div>
  </div>
  <!-- Row end -->
 
</div>


